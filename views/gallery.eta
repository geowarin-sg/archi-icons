<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Document</title>
    <style>
        svg {
            width: 100px;
            height: 100px;
            border: 1px solid black;
        }
        html {
            background-color: #145682;
        }
    </style>
    <title>Gallery</title>
</head>
<body>
<% for (let group of it.iconsData) { %>
<h1><%= group.dir %></h1>
<div style="display: flex; flex-direction: row; flex-wrap: wrap;">
    <% for (let icon of group.icons) { %>
    <div title="<%= icon.path %>"><%~ icon.content %></div>
    <% } %>
</div>
<% } %>
</body>
</html>